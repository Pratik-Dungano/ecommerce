{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://adaa-backend.vercel.app/api/:path*" }
  ]
  
}
